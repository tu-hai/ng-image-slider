{
  "name": "ngx-img-slider",
  "version": "7.0.2",
  "description": "Angular Image slider with lightbox. An Angular responsive image slider with lightbox popup. Also support youtube url, image base64 string and mp4 video urls and base64 string.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4100 --host 0.0.0.0",
    "build": "ng build ngx-img-slider --watch --configuration production",
    "build:prod": "ng build ngx-img-slider --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "files": [
    "dist/ngx-img-slider"
  ],
  "dependencies": {
    "@angular-devkit/schematics": "^15.0.4",
    "@angular/animations": "^15.0.4",
    "@angular/common": "^15.0.4",
